{
  "name": "hackeditr",
  "version": "0.0.1",
  "description": "Editor for Hackfoldr",
  "scripts": {
    "prebuild": "ln -sf ../node_modules app/node_modules",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "browserify -t browserify-css app/app.js -o app/bundle.js",
